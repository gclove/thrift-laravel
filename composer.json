{
    "name": "gclove/thrift-laravel",
    "description": "use Thrift in Laravel 5.6, forked from Angejia/thrift-laravel",
    "keywords": [
        "thrift",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "illuminate/http": "~5.6",
        "illuminate/support": "~5.6",
        "symfony/http-foundation": "4.0.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Gclove\\Thrift\\": "src/"
        }
    }
}
